{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/chunk-*.js",
          "/main-*.js",
          "/polyfills-*.js",
          "/runtime-*.js",
          "/offline.html"
        ]
      }
    },
    {
      "name": "public",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.svg",
          "/**/*.cur",
          "/**/*.jpg",
          "/**/*.jpeg",
          "/**/*.png",
          "/**/*.apng",
          "/**/*.webp",
          "/**/*.avif",
          "/**/*.gif",
          "/**/*.otf",
          "/**/*.ttf",
          "/**/*.woff",
          "/**/*.woff2"
        ]
      }
    }
  ]
}
