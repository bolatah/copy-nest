{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/offline.html" 
        ]
      }
    },
    {
      "name": "public",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }, 
    {
      "name": "public-offline",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/offline.html"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": ["https://your-api.com/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "1d",
        "timeout": "10s"
      }
    }
  ]
}
